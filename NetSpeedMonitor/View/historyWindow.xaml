<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:USTC.Software.hanyizhao.NetSpeedMonitor.View"
        xmlns:Medol="clr-namespace:USTC.Software.hanyizhao.NetSpeedMonitor.Medol" x:Class="USTC.Software.hanyizhao.NetSpeedMonitor.View.historyWindow"
        mc:Ignorable="d"
        Title="historyWindow" Height="294" Width="599">
    <Window.DataContext>
        <Medol:MedolHistory x:Name="Data"/>
    </Window.DataContext>
    <Grid>
        <Label Content="Total Upload :" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Label Content="Total Download :" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,31,0,0"/>
        <Label Content="{Binding Totaldownload,Mode=OneWay}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="103,0,0,0"/>
        <Label Content="{Binding Totalupload,Mode=OneWay}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="103,31,0,0"/>

    </Grid>
</Window>
